@article{Fisch2018,
abstract = {In an initial coin offering (ICO), new ventures raise capital by selling tokens to a crowd of investors. Often, this token is a cryptocurrency, a digital medium of value exchange based on blockchain technology. Both the number of ICOs and the amount of capital raised exploded in 2017. Despite attracting significant attention from ventures, investors, and policy makers, the blockchain sector is in its early stages, and little is known about the dynamics of ICOs. In this initial study, I therefore assess ICO's success factors by empirically exploring the capital raised in 238 ICOs in 2016 and 2017. The results show that several characteristics of the ICO campaign and the underlying technology determine the amount raised, while the venture characteristics are less important to investors. While some dynamics resemble those in the context of venture capital or crowdfunding, others are unique to the context of blockchain technology and have not been assessed before. Further analyses explore the duration of ICO campaigns as well as the language used in documents accompanying the ICO. The findings have important implications for theory and practice. For example, they inform policy makers about potential starting points for the introduction of ICO regulation.},
author = {Fisch, Christian},
doi = {10.2139/ssrn.3147521},
file = {:Users/javahsu/Library/Application Support/Mendeley Desktop/Downloaded/Fisch - 2018 - Initial Coin Offerings (ICOs) to Finance New Ventures An Exploratory Study(2).pdf:pdf},
