issn = {09291199},
journal = {Journal of Corporate Finance},
keywords = {IPOs,Indian IPOs,Manipulation,Pump-and-dump scheme,Trade-based manipulation},
pages = {115--130},
title = {{Trade-based manipulation: Beyond the prosecuted cases}},
