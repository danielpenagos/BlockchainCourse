keywords = {cryptocurrency trading,social media data mining,topic modelling},
pages = {80--86},
title = {{Mutual-Excitation of Cryptocurrency Market Returns and Social Media Topics}},
url = {http://dl.acm.org/citation.cfm?doid=3233347.3233370},
year = {2018}
