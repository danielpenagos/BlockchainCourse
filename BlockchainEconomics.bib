@article{Paquet-Clouston2019,
abstract = {Ransomware can prevent a user from accessing a device and its files until a ransom is paid to the attacker, most frequently in Bitcoin. With over 500 known ransomware families, it has become one of the dominant cybercrime threats for law enforcement, security professionals and the public. However, a more comprehensive, evidence-based picture on the global direct financial impact of ransomware attacks is still missing. In this paper, we present a data-driven method for identifying and gathering information on Bitcoin transactions related to illicit activity based on footprints left on the public Bitcoin blockchain. We implement this method on-top-of the GraphSense open-source platform and apply it to empirically analyze transactions related to 35 ransomware families. We estimate the lower bound direct financial impact of each ransomware family and find that, from 2013 to mid-2017, the market for ransomware payments has a minimum worth of USD 12,768,536 (22,967.54 BTC). We also find that the market is highly skewed with only a few number of players responsible for the majority of the payments. Based on these research findings, policy-makers and law enforcement agencies can use the statistics provided to understand the size of the illicit market and make informed decisions on how best to address the threat.},
archivePrefix = {arXiv},
arxivId = {1804.04080},
author = {Paquet-Clouston, Masarah and Haslhofer, Bernhard and Dupont, Beno{\^{i}}t},
doi = {10.1093/cybsec/tyz003},
eprint = {1804.04080},
file = {:Users/javahsu/Library/Application Support/Mendeley Desktop/Downloaded/Paquet-Clouston, Haslhofer, Dupont - 2019 - Ransomware payments in the Bitcoin ecosystem.pdf:pdf},
issn = {2057-2085},
journal = {Journal of Cybersecurity},
keywords = {Bitcoin,economics,graph analysis,ransomware},
month = {jan},
number = {1},
title = {{Ransomware payments in the Bitcoin ecosystem}},
url = {https://academic.oup.com/cybersecurity/article/doi/10.1093/cybsec/tyz003/5488907},
volume = {5},
year = {2019}
}
@article{Levchenko2011,
abstract = {Spam-based advertising is a business. While it has engendered both widespread antipathy and a multi-billion dollar anti-spam industry, it continues to exist because it fuels a profitable enterprise. We lack, however, a solid understanding of this enterprise's full structure, and thus most anti-spam interventions focus on only one facet of the overall spam value chain (e.g., spam filtering, URL blacklisting, site takedown). In this paper we present a holistic analysis that quantifies the full set of resources employed to monetize spam email-including naming, hosting, payment and fulfillment-using extensive measurements of three months of diverse spam data, broad crawling of naming and hosting infrastructures, and over 100 purchases from spam-advertised sites. We relate these resources to the organizations who administer them and then use this data to characterize the relative prospects for defensive interventions at each link in the spam value chain. In particular, we provide the first strong evidence of payment bottlenecks in the spam value chain; 95{\%} of spam-advertised pharmaceutical, replica and software products are monetized using merchant services from just a handful of banks.},
author = {Levchenko, K and Pitsillidis, A and Chachra, N and Enright, B and F{\'{e}}legyh{\'{a}}zi, M and Grier, C and Halvorson, T and Kanich, C and Kreibich, C and Liu, H and Others},
file = {:Users/javahsu/Library/Application Support/Mendeley Desktop/Downloaded/Levchenko et al. - 2011 - Click Trajectories End-to-End Analysis of the Spam Value Chain(2).pdf:pdf},
journal = {Proceedings of the 2011 ieee symposium on security and privacy},
title = {{Click Trajectories: End-to-End Analysis of the Spam Value Chain}},
url = {http://www.icir.org/christian/publications/2011-oakland-trajectory.pdf},
year = {2011}
}
